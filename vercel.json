{
  "version": 2,
  "builds": [
    {
      "src": "views/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "/public/login.html"
    },
    {
      "src": "/register",
      "dest": "/public/register.html"
    },
    {
      "src": "/dashboard",
      "dest": "/public/dashboard.html"
    },
    {
      "src": "/home",
      "dest": "/public/home.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
