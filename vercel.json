{
  "version": 2,
  "builds": [
    {
      "src": "src/routes/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1" // Serve static files from the public directory
    },
    {
      "src": "/(.*)",
      "dest": "src/routes/app.js"
    }
  ]
}
