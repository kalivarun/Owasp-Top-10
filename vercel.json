{
  "version": 2,
  "builds": [
    {
      "src": "views/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/views/server.js"
    }
  ]
}
